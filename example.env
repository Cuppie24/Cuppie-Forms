# Default Environment Configuration (Development)
NODE_ENV=development
ASPNETCORE_ENVIRONMENT=Development

# Frontend API URL
FRONTEND_API_URL=https://cuppie.api

# Backend Configuration
DATABASE_CONNECTION_STRING=Host=db;Port=5432;Database=database_name;Username=usernmae;Password=password
# CORS allowed origins separated by commas
CORS_ALLOWED_ORIGINS=https://cuppie.cup

# JWT Configuration
JWT__Key=super_secret_key
JWT__Audience=ApiUsers
JWT__Issuer=CuppieAuth
JWT__AccessTokenExpiresInMinutes=15
JWT__RefreshTokenExpiresInMinutes=10080

# Database Configuration
POSTGRES_DB=database_name
POSTGRES_USER=username
POSTGRES_PASSWORD=password